@using BsBios.Portal.ViewModel
@model AgendamentoDeCarregamentoCadastroVm
@{
    Layout = null;
}
<div class="divFiltros">
    <fieldset >
        <legend>Agendamento de Descarregamento</legend>
        @using(Html.BeginForm())
        {
            @Html.HiddenFor(x => x.IdQuota)
            @Html.HiddenFor(x => x.IdAgendamento)
            <div class="colunaFiltro">
                @Html.LabelFor(x => x.Placa)
                @Html.TextBoxFor(x => x.Placa)
                <p class="mensagemErro">
                    @Html.ValidationMessageFor(x => x.Placa)
                </p>
            </div>
            <div class="colunaFiltro">
                @Html.LabelFor(x => x.Peso)
                @Html.TextBoxFor(x => x.Peso)
                <p class="mensagemErro">
                    @Html.ValidationMessageFor(x => x.Peso)
                </p>
            </div>
        }
    </fieldset>
    <div id="divErro" class="message-error"></div>
</div>


